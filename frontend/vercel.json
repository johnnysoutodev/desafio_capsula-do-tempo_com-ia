{
  "version": 2,
  "buildCommand": "echo 'No build needed'",
  "outputDirectory": "frontend/public",
  "framework": null,
  "installCommand": "echo 'No install needed'",
  "builds": [
    {
      "src": "frontend/public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend/public/$1"
    }
  ]
}